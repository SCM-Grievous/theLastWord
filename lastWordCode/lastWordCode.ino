// stores the number of shots remaining before a reload is required
int shotsRemaining = 8;

// variables for trigger button switch
int triggerPin = 5;
bool triggerState = false;
bool prevTriggerState = false;

// variables for chamber magnet sensor
int magPin = 4;
bool magState = false;

// NeoPixel jewel object for muzzle flash
Adafruit_NeoPixel strip(7, 3);

// variables for tilt sensor
int tiltPin = 2;
bool tiltState = false;
bool prevTiltState = false;

/**
  Sets all pixels within a predefined Neopixel object to the given color
*/
void setAllColor(uint32_t color) {
  for (int i=0; i<strip.numPixels(); i++) {
    strip.setPixelColor(i, color);
  }

  strip.show();
}

void setup() {
  Serial.begin(19200);

  strip.begin();
  strip.clear();
  strip.show();
}

void loop() {
  // detect when trigger is pulled
  triggerState = digitalRead(triggerPin);

  // detect if weapon is tilted
  prevTiltState = tiltState;
  tiltState = digitalRead(tiltPin);

  // detect if chamber is closed
  magState = digitalRead(magPin);

  if (shotsRemaining <= 0) {
    Serial.println("Out of ammo");

    if (magState && (tiltState && !prevTiltState)) {
      Serial.println("Reloaded");
      shotsRemaining = 8;
    }

  } else if (triggerState && !magState) {
    shotsRemaining--;

    // muzzle flash
    setAllColor(strip.Color(255, 200, 0));
    Serial.println("Shot fired");

    // begin sound effect playback
    startPlayback(effect, sizeof(effect));

    // delay to mimic fire rate of weapon
    delay(100);
    setAllColor(strip.Color(0,0,0));
    delay(167);
  }
}

